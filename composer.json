{
    "name": "akbsit/laravel-system-file",
    "description": "Uploading files to the server.",
    "keywords": [
        "laravel",
        "media",
        "files",
        "images",
        "downloads",
        "cms",
        "upload"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "ext-fileinfo": "*",
        "laravel/framework": "^9.0|^10.0",
        "dusank/knapsack": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Akbsit\\SystemFile\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Akbsit\\SystemFile\\Providers\\MediaServiceProvider"
            ]
        }
    }
}
